{
  "name": "boilerplate-nodejs-typescript",
  "version": "0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cli:apikey": "ts-node src/scripts/generate_apikey.ts",
    "bash:generate-service": "/bin/bash ./scripts/create-service.sh",
    "bash:generate-keys": "/bin/bash ./src/scripts/generate_keys.sh",
    "lint": "eslint . --ext .js,.ts,.tsx --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.2",
    "bull": "4.6.2",
    "chalk": "4.1.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv-flow": "3.2.0",
    "express": "4.17.3",
    "glob": "7.2.0",
    "inquirer": "8.2.0",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "logplease": "1.2.15",
    "mongoose": "6.2.3",
    "morgan": "1.10.0",
    "node-mailjet": "3.3.7",
    "swagger-ui-express": "4.3.0",
    "uuid-apikey": "1.5.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/bull": "3.15.8",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/dotenv-flow": "3.2.0",
    "@types/express": "4.17.13",
    "@types/glob": "7.2.0",
    "@types/inquirer": "8.2.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/morgan": "1.9.3",
    "@types/node-mailjet": "3.3.8",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.4",
